{
  "template": {
    "settings": {
      "index": {
        "lifecycle": {
          "name": "fluentd"
        }
      }
    },
    "mappings": {
      "properties": {
        "log_duration": {
          "type": "integer"
        },
        "log_id": {
          "type": "text"
        },
        "log_pid": {
          "type": "long"
        },
        "log_size": {
          "type": "long"
        },
        "log_status": {
          "type": "integer"
        }
      }
    }
  },
  "index_patterns": [
    "fluentd-*"
  ]
}
