{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::{{ s3_bucket }}",
                "arn:aws:s3:::{{ s3_bucket }}/*",
                "arn:aws:s3:::{{ s3_backup_bucket }}",
                "arn:aws:s3:::{{ s3_backup_bucket }}/*",
                "arn:aws:s3:::{{ s3_logs }}",
                "arn:aws:s3:::{{ s3_logs }}/*"
            ]
        }
    ]
}
